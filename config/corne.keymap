        layer5 {
            bindings = <
                &none [[KC_RCBR]] [[KC_AMPR]] [[KC_ASTR]] [[KC_LPRN]] [[KC_LCBR]] &none &none &none &none [[QK_BOOT]] &none &none [[KC_DQUO]] [[KC_DLR]] [[KC_PERC]] [[KC_CIRC]] [[KC_PLUS]] &none &kp LEFT_SHIFT &kp LEFT_CONTROL &kp LEFT_ALT &kp LEFT_GUI &none &none [[KC_TILD]] [[KC_EXLM]] [[KC_AT]] [[KC_HASH]] [[KC_PIPE]] &none &none &none &kp RIGHT_ALT &none &none [[KC_LPRN]] [[KC_RPRN]] [[KC_UNDS]] &none &none &none
