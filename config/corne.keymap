        layer4 {
            bindings = <
                &none &kp RIGHT_BRACKET &kp N7 &kp N8 &kp N9 &kp LEFT_BRACKET &none &none &none &none [[QK_BOOT]] &none &none &kp SINGLE_QUOTE &kp N4 &kp N5 &kp N6 &kp EQUAL &none &kp LEFT_SHIFT &kp LEFT_CONTROL &kp LEFT_ALT &kp LEFT_GUI &none &none &kp GRAVE &kp N1 &kp N2 &kp N3 &kp BACKSLASH &none &none &none &kp RIGHT_ALT &none &none &kp DOT &kp N0 &kp MINUS &none &none &none
