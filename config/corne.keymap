        layer3 {
            bindings = <
                &none [[QK_BOOT]] &none &none &none &none [[RGB_TOG]] [[RGB_MOD]] [[RGB_HUI]] [[RGB_SAI]] [[RGB_VAI]] &none &none &kp LEFT_GUI &kp LEFT_ALT &kp LEFT_CONTROL &kp LEFT_SHIFT &none &kp C_PREVIOUS &kp C_VOLUME_DOWN &kp C_VOLUME_UP &kp C_NEXT [[RGB_VAD]] &none &none &none &kp RIGHT_ALT &none &none &none &none &kp C_BRIGHTNESS_DEC &kp C_BRIGHTNESS_INC &none &none &none &none &none &none &kp C_STOP &kp C_PLAY_PAUSE &kp C_MUTE
