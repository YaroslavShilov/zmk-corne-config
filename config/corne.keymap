#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/ext_power.h>

/ {
    keymap {
        compatible = "zmk,keymap";
        default_layer {
            bindings = <
                &kp GRAVE &kp Q &kp W &lt 7 E &kp R &kp T      &kp Y &kp U &kp I &kp O &kp P &kp LEFT_BRACKET
                &none &mt LEFT_GUI A &mt LEFT_ALT S &mt LEFT_CONTROL D &mt LEFT_SHIFT F &kp G &kp H &mt LEFT_SHIFT J &mt LEFT_CONTROL K &mt LEFT_ALT L &mt LEFT_GUI SEMICOLON &kp SINGLE_QUOTE
                &none &kp Z [[RALT_T(KC_X)]] &kp C &kp V &kp B &kp N &kp M &kp COMMA [[RALT_T(KC_DOT)]] &kp SLASH &kp RIGHT_BRACKET
                &lt 3 ESCAPE &lt 1 SPACE &lt 2 TAB &lt 5 ENTER &lt 4 BACKSPACE &lt 6 DELETE
            >;
        };
        layer1 {
            bindings = <
                &none [[QK_BOOT]] &none &none &none &none &kp LC(X) &kp LC(C) &kp LC(V) &kp LC(Z) &kp RC(RS(Z)) &kp K_AGAIN &none &kp LEFT_GUI &kp LEFT_ALT &kp LEFT_CONTROL &kp LEFT_SHIFT &none &kp LEFT &kp DOWN &kp UP &kp RIGHT &kp CAPSLOCK &none &none &none &kp RIGHT_ALT &none &none &none &kp HOME &kp PAGE_DOWN &kp PAGE_UP &kp END &kp INSERT &none &none &none &none &kp ENTER &kp BACKSPACE &kp DELETE
            >;
        };
        layer2 {
            bindings = <
                &none [[QK_BOOT]] &none &none &none &none &none &none &none &none &none &none &none &kp LEFT_GUI &kp LEFT_ALT &kp LEFT_CONTROL &kp LEFT_SHIFT &none [[KC_MS_L]] [[KC_MS_D]] [[KC_MS_U]] [[KC_MS_R]] &none &none &none &none &kp RIGHT_ALT &none &none &none [[KC_WH_L]] [[KC_WH_D]] [[KC_WH_U]] [[KC_WH_R]] &none &none &none &none &none [[KC_BTN2]] [[KC_BTN1]] [[KC_BTN3]]
            >;
